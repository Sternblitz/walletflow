NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL_HERE
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY_HERE

# Apple Wallet Configuration
# See docs/APPLE_SETUP.md for detailed setup instructions
APPLE_PASS_TYPE_ID=pass.com.passify.loyalty
APPLE_WWDR_CERT_PATH=./certs/AppleWWDRCA.cer
APPLE_SIGNER_CERT_PATH=./certs/passify-cert.p12
APPLE_SIGNER_KEY_PASSPHRASE=your_secure_password_here

# Google Wallet Configuration
# See docs/GOOGLE_SETUP.md for detailed setup instructions
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=./certs/google-service-account.json
GOOGLE_WALLET_CLASS_ID=passify_loyalty_default
GOOGLE_ISSUER_ID=your_issuer_id_here

# Optional: Base URL for Smart Links
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Gemini AI for Design Generation (Nano Banana AI)
GEMINI_API_KEY=your_gemini_api_key_here
